
    <%= render 'list', book: @book, user: current_user %>

  <div class="col-md-8 offset-md-1">
      <h1>Users</h1>

    <table class="table table-striped">
      <thead>
       <tr>
        <th>image</th>
         <th>name</th>
        <th></th>
       </tr>
      </thead>

      <tbody>
       <% @users.each do |user| %>
         <tr>
           <td><%= image_tag user.get_profile_image(50,50) %></td>
           <td><%= user.name %></td>
           <td><%= link_to "Show", user_path(user.id) %></td>
          </tr>
       <% end %>
      </tbody>
    </table>
  </div>
</div>
</div>